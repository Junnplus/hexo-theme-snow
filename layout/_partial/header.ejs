<% if (path=='') {%>
  <header id="header">
<%} else{%>
  <header id="header" class="small">
<%} %>
  <div id="header-outer" class="outer">
    <div id="header-title" class="inner">
      <h1 id="logo-wrap">
        <a href="<%- url_for() %>" id="logo" style="color:#b7b7b7;font-size:2em;text-decoration:none;border-bottom:thin dashed #222"><%= config.title %></a>
      </h1>
      <% if (theme.subtitle){ %>
        <h2 id="subtitle-wrap">
          <a href="<%- url_for() %>" id="subtitle"><%= theme.subtitle %></a>
        </h2>
      <% } %>
      <div class="right">
      <% if (theme.rss){ %>
        <a id="nav-rss-link" class="nav-icon" href="<%- theme.rss %>" title="RSS Feed"></a>
      <% } %>
      <% if (theme.github) {%>
        <a id="nav-github-link" class="nav-icon" href="https://github.com/<%- theme.github %>" title="Github" target="_blank"></a>
      <% } %>
      <% if (theme.twitter) {%>
        <a id="nav-twitter-link" class="nav-icon" href="https://twitter.com/<%- theme.twitter -%>" title="Twitter" target="_blank"></a>
      <% } %>
      <% if (theme.facebook) {%>
        <a id="nav-facebook-link" class="nav-icon" href="https://facebook.com/<%- theme.facebook -%>" title="Facebook" target="_blank"> </a>
      <% } %>
      <% if (theme.google_plus) {%>
        <a id="nav-google-plus-link" class="nav-icon" href="https://plus.google.com/u/0/<%- theme.google_plus -%>/posts" title="Google+" target="_blank"> </a>
      <% } %>
      <% if (theme.instagram) {%>
        <a id="nav-instagram-link" class="nav-icon" href="https://instagram.com/<%- theme.instagram -%>" title="Instagram" target="_blank"> </a>
      <% } %>
      <% if (theme.weibo) {%>
        <a id="nav-weibo-link" class="nav-icon" href="http://weibo.com/<%- theme.weibo -%>" title="Weibo" target="_blank"></a>
      <% } %>
        <a id="nav-archives-link" class="nav-icon" href="<%- url_for(theme.menu['archives']) %>" title="Archives"></a>
      </div>
    </div>
  </div>
  <div id="header-inner" class="">
    <a class="title"><h1><%= config.title %></h1></a>
    <!--nav id="main-nav">
      <a id="main-nav-toggle" class="nav-icon"></a>
    </nav-->
    <% if (theme.avatar) {%>
    <nav class="avatar">
      <a href="<%- url_for(theme.menu['about']) %>">
        <img src="<%- theme.avatar -%>" alt="">
      </a>
    </nav>
    <% } %>
    <nav id="sub-nav">
      <a id="nav-search-btn" class="nav-icon" title="Search"></a>
    </nav>
    <nav id="sub-nav" style="right:19px">
    </nav>
    <div id="search-form-wrap">
      <%- search_form({button: '&#xF002;'}) %>
    </div>
  </div>
  <div class="site-nav">
    <ul>
    <% for (var i in theme.menu){ %>
      <li>
        <a href="<%- url_for(theme.menu[i]) %>"><%= i %></a>
      </li>
    <% } %>
    </ul>
  </div>
</header>
